# ECE Core Environment Configuration
# Copy this file to .env and customize as needed

# Server Configuration
NODE_ENV=development
PORT=3000

# Dreamer Service Configuration
DREAM_INTERVAL_MS=900000
STARTUP_DELAY_MS=60000

# Logging Configuration
# Set to 'true' to suppress console output (useful for CLI integrations)
QWEN_CLI=false

# Memory Limits
MAX_FILE_SIZE=5242880
TOKEN_LIMIT=1000000

# Mirror Protocol
MIRROR_BATCH_SIZE=100
